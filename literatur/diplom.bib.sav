  nlm-unique-id          = {101513680},
  owner                  = {NLM},
  publication-status     = {epublish},
  revised                = {20181113},
  source                 = {Cold Spring Harb Perspect Biol. 2013 Jul 1;5(7):a010108. doi: 10.1101/cshperspect.a010108.},
