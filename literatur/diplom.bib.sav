  title-abbreviation     = {Mol Ecol},
}

@Article{liu_2010,
  author                 = {Liu, Yongchao and Schmidt, Bertil and Maskell, Douglas L.},
  title                  = {MSAProbs: multiple sequence alignment based on pair hidden Markov models and partition function posterior probabilities.},
  journal                = {Bioinformatics (Oxford, England)},
  year                   = {2010},
  volume                 = {26},
  pages                  = {1958-64},
  month                  = {Aug},
  abstract               = {MOTIVATION: Multiple sequence alignment is of central importance to bioinformatics and computational biology. Although a large number of algorithms for computing a multiple sequence alignment have been designed, the efficient computation of highly accurate multiple alignments is still a challenge. RESULTS: We present MSAProbs, a new and practical multiple alignment algorithm for protein sequences. The design of MSAProbs is based on a combination of pair hidden Markov models and partition functions to calculate posterior probabilities. Furthermore, two critical bioinformatics techniques, namely weighted probabilistic consistency transformation and weighted profile-profile alignment, are incorporated to improve alignment accuracy. Assessed using the popular benchmarks: BAliBASE, PREFAB, SABmark and OXBENCH, MSAProbs achieves statistically significant accuracy improvements over the existing top performing aligners, including ClustalW, MAFFT, MUSCLE, ProbCons and Probalign. Furthermore, MSAProbs is optimized for multi-core CPUs by employing a multi-threaded design, leading to a competitive execution time compared to other aligners. AVAILABILITY: The source code of MSAProbs, written in C++, is freely and publicly available from http://msaprobs.sourceforge.net.},
  address                = {England},
  article-doi            = {10.1093/bioinformatics/btq338},
  article-pii            = {btq338},
  completed              = {20101007},
  electronic-issn        = {1367-4811},
  electronic-publication = {20100623},
  history                = {2010/10/12 06:00 [medline]},
  issue                  = {16},
  keywords               = {*Algorithms, Amino Acid Sequence, Computational Biology/methods, Markov Chains, Probability, Sequence Alignment/*methods, Sequence Analysis, Protein/*methods, Software},
  language               = {english},
  linking-issn           = {1367-4803},
  location-id            = {10.1093/bioinformatics/btq338 [doi]},
  nlm-unique-id          = {9808944},
  owner                  = {NLM},
  publication-status     = {ppublish},
  revised                = {20100806},
  source                 = {Bioinformatics. 2010 Aug 15;26(16):1958-64. doi: 10.1093/bioinformatics/btq338. Epub 2010 Jun 23.},
  status                 = {MEDLINE},
  subset                 = {IM},
  title-abbreviation     = {Bioinformatics},
}

@Article{yoon_2009,
  author             = {Yoon, Byung-Jun},
  title              = {Hidden Markov Models and their Applications in Biological Sequence Analysis.},
  journal            = {Current genomics},
  year               = {2009},
  volume             = {10},
  pages              = {402-15},
  month              = {Sep},
  abstract           = {Hidden Markov models (HMMs) have been extensively used in biological sequence analysis. In this paper, we give a tutorial review of HMMs and their applications in a variety of problems in molecular biology. We especially focus on three types of HMMs: the profile-HMMs, pair-HMMs, and context-sensitive HMMs. We show how these HMMs can be used to solve various sequence analysis problems, such as pairwise and multiple sequence alignments, gene annotation, classification, similarity search, and many others.},
  article-doi        = {10.2174/138920209789177575},
  article-pii        = {CG-10-402},
  completed          = {20110714},
  electronic-issn    = {1875-5488},
  history            = {2010/03/02 06:01 [medline]},
  issue              = {6},
  keywords           = {Hidden Markov model (HMM), context-sensitive HMM (csHMM), pair-HMM, profile-HMM, profile-csHMM, sequence analysis.},
  language           = {english},
