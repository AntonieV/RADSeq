@Article{kuhner_2014,
  author                 = {Kuhner, Mary K. and McGill, James},
  title                  = {Correcting for sequencing error in maximum likelihood phylogeny inference.},
  journal                = {G3 (Bethesda, Md.)},
  year                   = {2014},
  volume                 = {4},
  pages                  = {2545-52},
  month                  = {Nov},
  abstract               = {Accurate phylogenies are critical to taxonomy as well as studies of speciation processes and other evolutionary patterns. Accurate branch lengths in phylogenies are critical for dating and rate measurements. Such accuracy may be jeopardized by unacknowledged sequencing error. We use simulated data to test a correction for DNA sequencing error in maximum likelihood phylogeny inference. Over a wide range of data polymorphism and true error rate, we found that correcting for sequencing error improves recovery of the branch lengths, even if the assumed error rate is up to twice the true error rate. Low error rates have little effect on recovery of the topology. When error is high, correction improves topological inference; however, when error is extremely high, using an assumed error rate greater than the true error rate leads to poor recovery of both topology and branch lengths. The error correction approach tested here was proposed in 2004 but has not been widely used, perhaps because researchers do not want to commit to an estimate of the error rate. This study shows that correction with an approximate error rate is generally preferable to ignoring the issue.},
  article-doi            = {10.1534/g3.114.014365},
  article-pii            = {GGG_014365},
  completed              = {20150820},
  electronic-issn        = {2160-1836},
  electronic-publication = {20141104},
  history                = {2015/08/21 06:00 [medline]},
  issue                  = {12},
  keywords               = {Animals, Computational Biology/*methods, Humans, Likelihood Functions, Models, Genetic, Models, Statistical, *Phylogeny, Probability, Sequence Analysis, DNA, *Software, maximum likelihood, phylogeny inference, sequencing error},
  language               = {english},
  linking-issn           = {2160-1836},
  location-id            = {10.1534/g3.114.014365 [doi]},
  nlm-unique-id          = {101566598},
  owner                  = {NLM},
  publication-status     = {epublish},
  revised                = {20181113},
  source                 = {G3 (Bethesda). 2014 Nov 4;4(12):2545-52. doi: 10.1534/g3.114.014365.},
  status                 = {MEDLINE},
  subset                 = {IM},
  termowner              = {NOTNLM},
  title-abbreviation     = {G3 (Bethesda)},
}

@Article{schirmer_2016,
  author   = {Schirmer, Melanie and D’Amore, Rosalinda and Ijaz, Umer Z. and Hall, Neil and Quince, Christopher},
  title    = {Illumina error profiles: resolving fine-scale variation in metagenomic sequencing data},
  journal  = {BMC Bioinformatics},
  year     = {2016},
  volume   = {17},
  number   = {1},
  pages    = {125},
  month    = mar,
  issn     = {1471-2105},
  abstract = {Illumina’s sequencing platforms are currently the most utilised sequencing systems worldwide. The technology has rapidly evolved over recent years and provides high throughput at low costs with increasing read-lengths and true paired-end reads. However, data from any sequencing technology contains noise and our understanding of the peculiarities and sequencing errors encountered in Illumina data has lagged behind this rapid development.},
  refid    = {Schirmer2016},
  url      = {https://doi.org/10.1186/s12859-016-0976-y},
}

