  keywords           = {Animals, DNA Replication/*genetics, Mammals, Replication Origin/*genetics, *DNA replication, *chromatin, *epigenetics, *nuclear architecture, *origins of DNA replication},
  language           = {english},
  linking-issn       = {0890-9369},
  location-id        = {10.1101/gad.285114.116 [doi]},
  nlm-unique-id      = {8711660},
  owner              = {NLM},
